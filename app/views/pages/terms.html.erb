<!DOCTYPE html>
<html lang="en">
<head>
</head>
<body data-spy="scroll" data-target=".fixed-top">

<!-- Preloader -->
<div class="spinner-wrapper">
  <div class="spinner">
    <div class="bounce1"></div>
    <div class="bounce2"></div>
    <div class="bounce3"></div>
  </div>
</div>
<!-- end of preloader -->
<!-- Navbar -->
<%= render  "layouts/navbar" %>
<!-- Header -->
<header id="header" class="ex-header">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <h1>Terms & Conditions</h1>
      </div> <!-- end of col -->
    </div> <!-- end of row -->
  </div> <!-- end of container -->
</header> <!-- end of ex-header -->
<!-- end of header -->
<% @term_and_conditions.each  do |term_condition| %>
  <div class="container">
    <div class="row">
      <div class="col-lg-12" style="margin-left:50px; font-size:20px;">
  <p> <%= term_condition.detail %> </p>
      </div> <!-- end of col -->
    </div> <!-- end of row -->
  </div>
<%end %>
<footer id="colophon" class="site-footer tg-site-footer ">
  <%= render "layouts/footer" %>
</footer>

</body>
</html>